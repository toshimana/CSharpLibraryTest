<Window x:Class="CSharpLibraryTest.Views.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:prism="http://prismlibrary.com/"
        xmlns:uc="clr-namespace:CSharpUserControlLibrary;assembly=CSharpUserControlLibrary"
        prism:ViewModelLocator.AutoWireViewModel="True"
        Title="{Binding Title.Value}" Height="350" Width="525" >
    <StackPanel HorizontalAlignment="Stretch">
        <uc:PathLoader Path.Value="{Binding ImagePath.Value}"/>
        <ContentControl prism:RegionManager.RegionName="ContentRegion" />
    </StackPanel>
</Window>
